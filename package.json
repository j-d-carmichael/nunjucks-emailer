{
  "name": "openapi-nodegen-emailer",
  "version": "1.0.1",
  "description": "A set of files to get moving for a typescript npm package, remove all not required",
  "licence": "MIT",
  "main": "./build/index.js",
  "scripts": {
    "install:githooks": "cp ./githooks/* .git/hooks",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "test:unit:single": "jest --collect-coverage false",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "build:tocs": "npx doctoc .",
    "build": "rm -fR ./build && ttsc -p . -d --declarationDir build",
    "build:watch": "ttsc --watch -p .",
    "start": "node cli.js",
    "prepublishOnly": "npm run lint && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johndcarmichael/openapi-nodegen-emailer.git"
  },
  "keywords": [
    "nunjucks",
    "email",
    "sendgrid"
  ],
  "peerDependencies": {
    "@sendgrid/mail": "^6.4.0"
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
    "nunjucks": "^3.2.0"
  },
  "devDependencies": {
    "@sendgrid/mail": "^6.4.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "@types/nunjucks": "^3.1.3",
    "@zerollup/ts-transform-paths": "^1.7.9",
    "codecov": "^3.6.1",
    "eslint": "^6.8.0",
    "hasha": "^5.1.0",
    "jest": "^24.9.0",
    "recursive-readdir-sync": "^1.0.6",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "ttypescript": "^1.5.8",
    "typescript": "^3.7.4"
  }
}
